package pack1;

public class Depart {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("helloworld");
		// double resultat = photocopie(10);
		futur(10, 75);
		// System.out.println("Resultat = " + resultat);
	}

	static void futur(int h, int m) {
		m = m + 1;
		if (m > 59) {
			h += m / 60;
			m = m % 60;

		}
		if (h > 23) {
			h = h % 24;
		}
		System.out.println(h + ":" + m);
	}
}
/*
 * static double photocopie(int nbPhoto) { if (nbPhoto <= 10) { return nbPhoto *
 * 0.1; }
 * 
 * else if (nbPhoto <= 30) {
 * 
 * return 1 + (nbPhoto - 10) * 0.09;
 * 
 * }
 * 
 * else { return 2.8 + (nbPhoto - 30) * 0.08;
 * 
 * } }
 * 
 * }
 */
